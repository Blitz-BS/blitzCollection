{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/Blitz-BS/blitzCollection/refs/heads/MGP-421-modularisation-de-l-objet-json-schema-pour-l-integ/json_schema/gps.schema.json",
  "title": "GPS",
  "description": "Coordonnées GPS",
  "required": [ "latitude", "longitude" ],
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "$schema": {
      "title" : "Schéma JSON",
      "description" : "URL du schéma",
      "type": "string",
      "format": "uri"
    },
    "latitude": {
      "title" : "Latitude",
      "Description" : "Latitude en degrés décimaux",
      "type": "number",
      "minimum": -90,
      "maximum": 90
    },
    "longitude": {
      "title" : "Longitude",
      "Description" : "Longitude en degrés décimaux",
      "type": "number",
      "minimum": -180,
      "maximum": 180
    }
  }
}